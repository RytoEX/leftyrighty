{
  "manifest_version": 2,

  "name": "LeftyRighty",
  "description": "Navigate to the next or previous page.",
  "version": "0.0.1",

  "background": { "scripts": ["background.js"] },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],

  "icons": { "16": "./icons/icon16.png",
           "48": "./icons/icon48.png",
          "128": "./icons/icon128.png" },

  "permissions": ["tabs"]
}
